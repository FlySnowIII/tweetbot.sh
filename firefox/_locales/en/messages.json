{
  "extensionName": { "message": "tweetbot.sh remote controller" },
  "extensionDescription": { "message": "Provide context menu commands to share tweets by tweetbot.sh" },

  "contextMenu.fav.label": { "message": "Favorite this" },
  "contextMenu.unfav.label": { "message": "Unfavorite this" },
  "contextMenu.rt.label": { "message": "RT this" },
  "contextMenu.unrt.label": { "message": "Cancel RT of this" },
  "contextMenu.rt-now.label": { "message": "RT this immediately" },
  "contextMenu.fav-and-rt.label": { "message": "Favorite and RT this" },
  "contextMenu.fav-and-rt-now.label": { "message": "Favorite and RT this immediately" },
  "contextMenu.follow.label": { "message": "Follow" },
  "contextMenu.unfollow.label": { "message": "Unfollow" },

  "notConfiguredError.title": { "message": "Not configured!" },
  "notConfiguredError.message": { "message": "Please set path to the tweet.sh and the screen name of the account to be controlled." },

  "notTweetError.title": { "message": "Not a tweet!" },
  "notTweetError.message": { "message": "$URL$ is not a tweet.",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://..."
      }
    }
  },

  "onResponse.title": { "message": "Operation succeeded." },
  "onResponse.message": { "message": "Command successfully sent: $COMMAND$",
    "placeholders": {
      "command": {
        "content": "$1",
        "example": "fav xxx"
      }
    }
  },

  "onError.title": { "message": "Operation failed." },
  "onError.message": { "message": "Command: $COMMAND$\nError: $ERROR$",
    "placeholders": {
      "command": {
        "content": "$1",
        "example": "fav xxx"
      },
      "error": {
        "content": "$2",
        "example": "???"
      }
    }
  },

  "config.tweetsh_path.label.before": { "message": "Full path to the tweet.sh:" },
  "config.tweetjstweetsh_path_path.label.after": { "message": "" },
  "config.target_account.label.before": { "message": "Screnen name of the bot account to be controlled:" },
  "config.target_account.label.after": { "message": "" },
  "config.debug.label": { "message": "Print debug log" }
}
